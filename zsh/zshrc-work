unalias mci
unalias gbweb
unalias gbbo3

alias mci='mvn clean install -P jboss-deployment'
alias gbweb='./gradlew buildApp -Dspring.profiles.active=prod'
alias gbbo3='./gradlew buildApp -Pdisable-tomcat'
alias gbaps='./gradlew buildApp -Pdisable-tomcat'
alias gwb='./gradlew build'
alias gwbr='./gradlew clean build bootRun'
alias br='./buildRun.sh'
alias r='./run.sh'

alias gafzf='git ls-files -m -o --exclude-standard | fzf -m | xargs git add'
